{
	"manifest_version": 2,
	"name": "Dial It",
	"short_name": "dial",
	"description": "Dials a selected number",
	"version": "0.0.1",
	"minimum_chrome_version" : "38",
	"permissions": [
		"contextMenus",
		"webRequest",
		"webRequestBlocking",
		"storage",
		"tabs",
		"<all_urls>"
	],
	"icons": {
		"16": "assets/dial.png"
	},
	"options_ui": {
		"page": "options.html" ,
		"open_in_tab": true
	},
	"content_security_policy": "script-src 'self'; object-src 'self'",
	"background": {"page": "background.html"}
}
